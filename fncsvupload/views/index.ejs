<!DOCTYPE html>
<html lang="en">
<head>
  <title></title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
 <style type="text/css">
     table, td, th {  
     border: 1px solid #ddd;
     text-align: left;
   }
   
   table {
     border-collapse: collapse;
     width: 50%;
    
   }
   .table-data{
       position: relative;
     left:50px;
     top:50px;
   }
   th, td {
     padding: 15px;
   }
 </style>
</head>
<body>
  <div class="btn">
    
  </div>
<!--====form section start====-->
    <div class="table-data">
    <table border="1" >
        <tr>
            <th>S.N</th>
            <th>Medicine Name</th>
            <th>Batch Number</th>
            <th>Ex Date/th>
            <th>Qty</th>
            <th>Shema</th>
            <th>MRP</th>
            <th>Manuf</th>
            <th>Hsn Code</th>
        </tr>
        <%
        if(medicineData.length!=0){
        var i=1;
        medicineData.forEach(function(data){
        %>
        <tr>
            <td><%=i; %></td>
            <td><%=data.c_name %></td>
            <td><%=data.c_batch_no %></td>
            <td><%=data.d_expiry_date%></td>
            <td><%=data.n_balance_qty %></td>
            <td><%=data.c_packaging %></td>
            <td><%=data.c_unique_code %></td>
            <td><%=data.c_schemes %></td>
            <td><%=data.n_mrp%></td>
            <td><%=data.c_manufacturer %></td>
            <td><%=data.hsn_code %></td>
        </tr>
        <%  i++; }) %>
        <% } else{ %>
            <tr>
                <td colspan="7">No Data Found</td>
            </tr>
        <% } %>
        
    </table>
    </div>
</body>
</html>
